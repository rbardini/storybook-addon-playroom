{
  "presets": [
    [
      "@babel/preset-env",
      { "shippedProposals": true, "useBuiltIns": "usage", "corejs": "3" }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["@babel/plugin-proposal-private-methods", { "loose": true }]
  ]
}
